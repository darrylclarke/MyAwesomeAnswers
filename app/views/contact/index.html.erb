<h1>Contact Us!</h1>


<%= form_tag "/contact" do %>
<div><%= label_tag "Name:" %>
<%= text_field_tag :name %>
</div
<%= label_tag "Email:" %>
<%= email_field_tag :email %>
<%= submit_tag "Contact Us" %>

<% end %>

